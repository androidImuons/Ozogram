<?xml version="1.0" encoding="utf-8"?>
<layout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

        <variable
            name="userprofiel"
            type="com.ozonetech.ozogram.viewmodel.UserProfileResponseModel" />


    </data>


    <FrameLayout
        android:id="@+id/fl_storyFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/colorWhite"
        tools:context=".view.fragment.ViewStoryFragment">

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/rv_profile_story_gallery"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/dp200"
            tools:listitem="@layout/post_row_item" />

    </FrameLayout>
</layout>

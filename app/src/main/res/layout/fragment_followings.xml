<?xml version="1.0" encoding="utf-8"?>
<layout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable
            name="followingsModel"
            type="com.ozonetech.ozogram.viewmodel.FollowerResponseModel" />
    </data>

    <FrameLayout
        android:id="@+id/fl_followingFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/colorWhite"
        tools:context=".view.fragment.FollowingsFragment">

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/rv_following_list"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/dp20"
            tools:listitem="@layout/following_item_layout" />

    </FrameLayout>
</layout>
